@model List<GetPopularBooksDto>
<div id="history" data-tab-content>
	<div class="row">
		@foreach (var item in Model)
		{
			<div class="col-md-3">
				<div class="product-item">
					<figure class="product-style">
						<img src="@item.ImageUrl" style="height:400px; width: auto!important" alt="Books" class="product-item">
						<button type="button" class="add-to-cart" data-product-tile="add-to-cart">
							@item.CategoryName
						</button>
					</figure>
					<figcaption>
						<h3>@item.BookName</h3>
						<span>@item.WriterName</span>
					</figcaption>
				</div>
			</div>
		}
	</div>
</div>
